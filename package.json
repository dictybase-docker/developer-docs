{
  "name": "deployment",
  "private": true,
  "description": "A simple Gatsby site for displaying dictyBase developer documentation.",
  "version": "0.1.0",
  "author": "Eric Hartline <eric.hartline@northwestern.edu>",
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "gatsby": "^2.22.15",
    "gatsby-image": "^2.4.4",
    "gatsby-plugin-manifest": "^2.4.5",
    "gatsby-plugin-material-ui": "^2.1.9",
    "gatsby-plugin-offline": "^3.2.3",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.4",
    "gatsby-remark-code-buttons": "^2.0.4",
    "gatsby-remark-images": "^3.3.8",
    "gatsby-source-filesystem": "^2.3.4",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.0.0",
    "gh-pages": "^3.0.0",
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -r https://$GH_TOKEN@github.com/dictybase-docker/developer-docs.git",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dictybase-docker/developer-docs"
  },
  "bugs": {
    "url": "https://github.com/dictybase-docker/developer-docs/issues"
  }
}
