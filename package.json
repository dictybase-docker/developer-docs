{
  "name": "deployment",
  "private": true,
  "description": "A simple Gatsby site for displaying dictyBase developer documentation.",
  "version": "0.1.0",
  "author": "Eric Hartline <eric.hartline@northwestern.edu>",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "gatsby": "^2.24.66",
    "gatsby-image": "^2.4.19",
    "gatsby-plugin-manifest": "^2.4.32",
    "gatsby-plugin-material-ui": "^2.1.9",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-remark-autolink-headers": "^2.3.11",
    "gatsby-remark-code-buttons": "^2.0.4",
    "gatsby-remark-images": "^3.3.30",
    "gatsby-remark-prismjs": "^3.5.13",
    "gatsby-remark-table-of-contents": "^0.1.1",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-remark": "^2.8.35",
    "gatsby-transformer-sharp": "^2.5.11",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "gh-pages": "^3.1.0",
    "prettier": "2.1.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -r https://$GH_TOKEN@github.com/dictybase-docker/developer-docs.git",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dictybase-docker/developer-docs"
  },
  "bugs": {
    "url": "https://github.com/dictybase-docker/developer-docs/issues"
  }
}